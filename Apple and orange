#include <stdio.h>

void countApplesAndOranges(int s, int t, int a, int b, int apples[], int m, int oranges[], int n) {
    int appleCount = 0, orangeCount = 0;

    // Count apples
    for (int i = 0; i < m; i++) {
        int landingPos = a + apples[i];
        if (landingPos >= s && landingPos <= t) {
            appleCount++;
        }
    }

    // Count oranges
    for (int i = 0; i < n; i++) {
        int landingPos = b + oranges[i];
        if (landingPos >= s && landingPos <= t) {
            orangeCount++;
        }
    }

    printf("%d\n", appleCount);
    printf("%d\n", orangeCount);
}

int main() {
    int s, t;
    scanf("%d %d", &s, &t);

    int a, b;
    scanf("%d %d", &a, &b);

    int m, n;
    scanf("%d %d", &m, &n);

    int apples[m], oranges[n];

    for (int i = 0; i < m; i++)
        scanf("%d", &apples[i]);

    for (int i = 0; i < n; i++)
        scanf("%d", &oranges[i]);

    countApplesAndOranges(s, t, a, b, apples, m, oranges, n);

    return 0;
}
