#include <stdio.h>
#include <stdlib.h>

// Function to reverse a number
int reverse(int n) {
    int rev = 0;
    while (n > 0) {
        rev = rev * 10 + (n % 10);
        n /= 10;
    }
    return rev;
}

int beautifulDays(int i, int j, int k) {
    int count = 0;
    for (int day = i; day <= j; day++) {
        int revDay = reverse(day);
        int diff = abs(day - revDay);
        if (diff % k == 0)
            count++;
    }
    return count;
}

int main() {
    int i, j, k;
    scanf("%d %d %d", &i, &j, &k);

    int result = beautifulDays(i, j, k);
    printf("%d\n", result);

    return 0;
}
